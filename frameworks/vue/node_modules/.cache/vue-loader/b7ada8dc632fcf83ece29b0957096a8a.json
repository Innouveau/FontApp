\n                if (this.musicIsPlaying) {\n                    this.play();\n                }\n            },\n            play() {\n                this.audio.play();\n            },\n            stop() {\n                this.audio.pause();\n            }\n        },\n        mounted() {\n            this.init();\n        }\n    }\n</script>\n\n\n<template>\n    <div\n        @click=\"toggleMusic()\"\n        class=\"tool-button\">\n        <img src=\"assets/icons/music.svg\">\n    </div>\n</template>\n\n\n<style lang=\"scss\">\n    @import '@/styles/variables.scss';\n\n    .tool-music {\n\n    }\n</style>"]}]}