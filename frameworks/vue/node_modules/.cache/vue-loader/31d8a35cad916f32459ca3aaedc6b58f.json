                  <div class=\"user__contact-label\">\n                        {{translate('email', true)}}\n                    </div>\n                    <div class=\"user__contact-value\">\n                        <a :href=\"'mailto:' + user.email\" target=\"_blank\">{{user.email}}</a>\n                    </div>\n                </div>\n                <div class=\"user__contact-row\">\n                    <div class=\"user__contact-label\">\n                        {{translate('telephone', true)}}\n                    </div>\n                    <div class=\"user__contact-value\">\n                        {{user.telephone}}\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n\n<style lang=\"scss\">\n    @import '@/styles/variables.scss';\n\n    .user {\n        display: flex;\n\n        .user__image {\n            margin-right: 50px;\n            border-radius: 11px;\n            width: 250px;\n            box-shadow: 3px 3px 3px #0000002E;\n            overflow: hidden;\n\n            img {\n                display: block;\n                width: 100%;\n            }\n        }\n\n        .user__text {\n            width: calc(100% - 250px - 50px);\n\n            .user__name {\n                font-weight: 700;\n                color: $main-blue;\n                font-size: 30px;\n                margin-bottom: 8px;\n            }\n\n            .user__description {\n                margin-bottom: $standard-padding-x2;\n            }\n\n            .user__contact {\n\n                .user__contact-row {\n                    display: flex;\n\n                    .user__contact-label {\n                        width: 60px;\n                        color: $main-blue;\n                        font-weight: 700;\n                    }\n\n                    .user__contact-value {\n                        width: calc(100% - 60px);\n                    }\n                }\n            }\n        }\n\n        @include mobile() {\n\n            .user__image {\n                margin-right: 50px;\n                width: 140px;\n            }\n\n            .user__text {\n                width: calc(100% - 140px - 50px);\n\n                .user__name {\n                    font-size: 18px;\n                    margin-bottom: 4px;\n                }\n            }\n        }\n    }\n</style>"]}]}