Content":["<script>\n    import slideTools from '@/tools/slide';\n\n    export default {\n        name: 'slide-current',\n        components: {},\n        props: {},\n        computed: {\n            currentSlide() {\n                return this.$store.state.slides.current;\n            },\n            slides(){\n                return this.$store.state.slides.all;\n            },\n            indexOfCurrentSlide() {\n                return this.slides.indexOf(this.currentSlide);\n            },\n            length() {\n                return this.slides.length;\n            }\n        },\n        methods: {\n            nextSlide() {\n                let i;\n                i = this.indexOfCurrentSlide + 1;\n                if (i > this.length - 1) {\n                    i = 0;\n                }\n                this.goto(i);\n\n            },\n            previousSlide() {\n                let i;\n                i = this.indexOfCurrentSlide - 1;\n                if (i < 0) {\n                    i = this.length - 1;\n                }\n                this.goto(i);\n            },\n            goto(i) {\n                let slide = this.$store.state.slides.all[i];\n                this.$store.commit('slides/setCurrent', slide);\n                slideTools.select(slide);\n            },\n            toggleSlides() {\n                this.$store.commit('ui/updateProperty', {key: 'slides', value: !this.$store.state.ui.slides});\n            }\n        }\n    }\n</script>\n\n\n<template>\n    <div class=\"slide-current slide\">\n        <div\n            id=\"direction-9\"\n            @click=\"previousSlide()\"\n            class=\"slide__button\">\n            <img src=\"assets/icons/triangle-left.svg\">\n        </div>\n        <div\n            @click=\"toggleSlides()\"\n            class=\"slide__title\">\n            {{currentSlide.title}}\n        </div>\n        <div\n            @click=\"nextSlide()\"\n            class=\"slide__button\">\n            <img src=\"assets/icons/triangle-right.svg\">\n        </div>\n    </div>\n</template>\n\n\n<style lang=\"scss\">\n    @import '@/styles/variables.scss';\n\n    .slide-current {\n        justify-content: space-between;\n\n        .slide__button {\n            height: 100%;\n            cursor: pointer;\n            width: $slide-button;\n\n            img {\n                height: 100%;\n            }\n        }\n\n        .slide__title {\n            flex-grow: 1;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            cursor: pointer;\n            height: 100%;\n        }\n    }\n</style>"]}]}